<div class="panel panel-default">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Nome Jurídico</th>
        <th>Nome Fantasia</th>
        <th>CNPJ</th>
        <th>Tipo</th>
        <th>Denominação social</th>
        <th>Situação</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @companies.each do |company| %>
      <tr>
        <td>
          <%= link_to company.name, company_path(company), class: "btn btn-link" %>  
        </td>
        <td><%= company.fantasy_name %></td>
        <td><%= company.cnpj %></td>
        <td><%= company.tipo %>
        <br><%= company.social_denomination %></td>
        <td><%= company.situation %></td>                          
      </tr>
      <% end %>
    </tbody>
  </table>
</div>