<p id="notice"><%= notice %></p>

<h1>Listing Second Levels</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Balance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @second_levels.each do |second_level| %>
      <tr>
        <td><%= second_level.code %></td>
        <td><%= second_level.name %></td>
        <td><%= second_level.balance %></td>
        <td><%= link_to 'Show', second_level %></td>
        <td><%= link_to 'Edit', edit_second_level_path(second_level) %></td>
        <td><%= link_to 'Destroy', second_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Second level', new_second_level_path %>
