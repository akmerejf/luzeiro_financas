<p id="notice"><%= notice %></p>

<h1>Listing Third Levels</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Second level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @third_levels.each do |third_level| %>
      <tr>
        <td><%= third_level.code %></td>
        <td><%= third_level.name %></td>
        <td><%= third_level.second_level %></td>
        <td><%= link_to 'Show', third_level %></td>
        <td><%= link_to 'Edit', edit_third_level_path(third_level) %></td>
        <td><%= link_to 'Destroy', third_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Third level', new_third_level_path %>
